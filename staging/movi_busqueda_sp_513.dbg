SELECT sp.NUEMP, sp.RFC, sp.CURP, sp.NOMBRE, sp.PUESTO, sp.NUMPTO, nvl((SELECT DESCRIPCION FROM MOVI_PUESTOS WHERE CODIGO=sp.PUESTO),'') DESCRIPCION_PUESTO , nvl((SELECT RAMA FROM MOVI_PUESTOS WHERE CODIGO=sp.PUESTO),'') RAMA,  nvl((SELECT DESCRIPCION FROM MOVI_UR WHERE ur='513'),'') DESCRIPCION_UR FROM MOVI_SERVIDORES_PUBLICOS sp where sp.ur='513' and  (sp.rfc like '%CARLOS%' or sp.CURP like '%CARLOS%'  or sp.nombre like '%CARLOS%' or sp.puesto like '%CARLOS%' )  and (sp.rfc like '%MEMEHUA%' or sp.CURP like '%MEMEHUA%'  or sp.nombre like '%MEMEHUA%' or sp.puesto like '%MEMEHUA%' )ORDER BY  sp.RFC, sp.CURP, sp.NOMBRE, sp.PUESTO, sp.NUMPTO OFFSET 0 ROWS FETCH NEXT 9 ROWS ONLY 